    0% { transform: translate(0, 0); }
    100% { transform: translate(60px, 60px); }
  }

  /* Interactive Floating Orbs */
  .floating-orbs {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    pointer-events: none;
    z-index: 1;
  }

  .orb {
    position: absolute;
    border-radius: 50%;
    filter: blur(2px);
    opacity: 0.4;
    animation: floatOrb 8s ease-in-out infinite;
  }

  .orb-1 {
    width: 200px;
    height: 200px;
    background: radial-gradient(circle, rgba(30, 58, 95, 0.15) 0%, transparent 70%);
    top: 20%;
    right: 15%;
    animation-delay: 0s;
  }

  .orb-2 {
    width: 150px;
    height: 150px;
    background: radial-gradient(circle, rgba(139, 145, 153, 0.2) 0%, transparent 70%);
    bottom: 30%;
    left: 10%;
    animation-delay: -3s;
  }

  .orb-3 {
    width: 120px;
    height: 120px;
    background: radial-gradient(circle, rgba(30, 58, 95, 0.1) 0%, transparent 70%);
    top: 60%;
    right: 40%;
    animation-delay: -6s;
  }

  .orb-4 {
    width: 180px;
    height: 180px;
    background: radial-gradient(circle, rgba(139, 145, 153, 0.15) 0%, transparent 70%);
    top: 10%;
    left: 20%;
    animation-delay: -9s;
  }

  @keyframes floatOrb {
    0%, 100% { transform: translate(0, 0) scale(1); }
    25% { transform: translate(20px, -30px) scale(1.1); }
    50% { transform: translate(-15px, -20px) scale(0.9); }
    75% { transform: translate(25px, 10px) scale(1.05); }
  }

  /* Particle System */
  .particle-container {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    overflow: hidden;
    pointer-events: none;
    z-index: 1;
  }

  .particle {
    position: absolute;
    width: 3px;
    height: 3px;
    background: rgba(30, 58, 95, 0.3);
    border-radius: 50%;
    animation: particle-float 6s linear infinite;
  }

  @keyframes particle-float {
    0% {
      opacity: 0;
      transform: translateY(100vh) scale(0);
    }
    10% {
      opacity: 1;
      transform: translateY(90vh) scale(1);
    }
    90% {
      opacity: 1;
      transform: translateY(-10vh) scale(1);
    }
    100% {
      opacity: 0;
      transform: translateY(-20vh) scale(0);
    }
  }

  /* Enhanced Card Interactions */
  .card-feature {
    position: relative;
    transition: all 0.4s cubic-bezier(0.25, 0.46, 0.45, 0.94);
  }

  .card-feature::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: linear-gradient(135deg, rgba(30, 58, 95, 0.05), rgba(139, 145, 153, 0.05));
    border-radius: inherit;
    opacity: 0;
    transition: opacity 0.3s ease;
    z-index: -1;
  }

  .card-feature:hover {
    transform: translateY(-12px) scale(1.02);
    box-shadow: 0 25px 50px -12px rgba(30, 58, 95, 0.25);
  }

  .card-feature:hover::before {
    opacity: 1;
  }

  /* Glassmorphism enhancement */
  .glass {
    backdrop-filter: blur(20px);
    -webkit-backdrop-filter: blur(20px);
    border: 1px solid rgba(255, 255, 255, 0.125);
  }

  /* Enhanced Typography */
  .gradient-sba {
    background: linear-gradient(135deg, #1e3a5f 0%, #2d4a6b 50%, #8b9199 100%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
    position: relative;
  }

  .gradient-sba::after {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: linear-gradient(135deg, #1e3a5f 0%, #2d4a6b 50%, #8b9199 100%);
    -webkit-background-clip: text;
    background-clip: text;
    animation: shimmer 3s ease-in-out infinite;
    opacity: 0;
  }

  @keyframes shimmer {
    0%, 100% { opacity: 0; }
    50% { opacity: 0.3; }
  }

  /* Micro-interactions for metrics */
  .metric-item {
    cursor: pointer;
    transition: all 0.3s cubic-bezier(0.25, 0.46, 0.45, 0.94);
  }

  .metric-item:hover {
    transform: translateY(-5px);
  }

  .metric-item:hover .metric-value {
    animation: pulse-glow 1s ease-in-out;
  }

  @keyframes pulse-glow {
    0%, 100% { text-shadow: none; }
    50% { text-shadow: 0 0 20px rgba(30, 58, 95, 0.5); }
  }

  /* Advanced button effects */
  .btn-primary, .btn-secondary {
    position: relative;
    overflow: hidden;
  }

  .btn-primary::before, .btn-secondary::before {
    content: '';
    position: absolute;
    top: 50%;
    left: 50%;
    width: 0;
    height: 0;
    background: rgba(255, 255, 255, 0.2);
    border-radius: 50%;
    transform: translate(-50%, -50%);
    transition: width 0.6s, height 0.6s;
  }

  .btn-primary:hover::before, .btn-secondary:hover::before {
    width: 300px;
    height: 300px;
  }
</style>

<script>
  // Advanced Interactive Effects System
  document.addEventListener('DOMContentLoaded', () => {

    // Particle System
    function createParticleSystem() {
      const container = document.getElementById('particles');
      if (!container) return;

      function createParticle() {
        const particle = document.createElement('div');
        particle.className = 'particle';

        // Random horizontal position
        particle.style.left = Math.random() * 100 + '%';

        // Random animation duration
        particle.style.animationDuration = (Math.random() * 4 + 4) + 's';

        // Random delay
        particle.style.animationDelay = Math.random() * 2 + 's';

        if (container) container.appendChild(particle);

        // Remove after animation
        setTimeout(() => {
          if (particle.parentNode) {
            particle.parentNode.removeChild(particle);
          }
        }, 8000);
      }

      // Create particles periodically
      setInterval(createParticle, 1500);

      // Create initial particles
      for (let i = 0; i < 5; i++) {
        setTimeout(createParticle, i * 300);
      }
    }

    // Mouse-following effects for orbs
    function initMouseEffects() {
      let mouseX = 0;
      let mouseY = 0;

      document.addEventListener('mousemove', (e) => {
        mouseX = e.clientX / window.innerWidth;
        mouseY = e.clientY / window.innerHeight;

        const orbs = document.querySelectorAll('.orb');
        orbs.forEach((orb, index) => {
          const factor = (index + 1) * 0.02;
          const x = (mouseX - 0.5) * factor * 100;
          const y = (mouseY - 0.5) * factor * 100;

          (orb as HTMLElement).style.transform = `translate(${x}px, ${y}px)`;
        });
      });
    }

    // Advanced scroll-triggered animations
    function initScrollAnimations() {
      const observerOptions = {
        threshold: 0.1,
        rootMargin: '0px 0px -50px 0px'
      };

      const observer = new IntersectionObserver((entries) => {
        entries.forEach((entry) => {
          if (entry.isIntersecting) {
            entry.target.classList.add('animate-fade-in');

            // Add staggered animations for children
            const children = entry.target.querySelectorAll('.card-feature, .metric-item');
            children.forEach((child, index) => {
              setTimeout(() => {
                (child as HTMLElement).style.opacity = '1';
                (child as HTMLElement).style.transform = 'translateY(0)';
              }, index * 100);
            });
          }
        });
      }, observerOptions);

      // Observe elements for animation
      const animateElements = document.querySelectorAll('.animate-on-scroll, .card-feature, .glass, section');
      animateElements.forEach((el) => observer.observe(el));
    }

    // Magnetic button effects
    function initMagneticButtons() {
      const buttons = document.querySelectorAll('.btn-primary, .btn-secondary');

      buttons.forEach(button => {
        button.addEventListener('mouseenter', function(e) {
          const rect = this.getBoundingClientRect();
          const relX = e.clientX - rect.left;
          const relY = e.clientY - rect.top;

          this.style.transform = `translate(${(relX - rect.width/2) * 0.1}px, ${(relY - rect.height/2) * 0.1}px) scale(1.05)`;
        });

        button.addEventListener('mouseleave', function() {
          this.style.transform = 'translate(0, 0) scale(1)';
        });

        button.addEventListener('mousemove', function(e) {
          const rect = this.getBoundingClientRect();
          const relX = e.clientX - rect.left;
          const relY = e.clientY - rect.top;

          this.style.transform = `translate(${(relX - rect.width/2) * 0.1}px, ${(relY - rect.height/2) * 0.1}px) scale(1.05)`;
        });
      });
    }

    // Counter animation for metrics
    function animateCounters() {
      const counters = document.querySelectorAll('.metric-value');

      const animateValue = (obj, start, end, duration) => {
        let startTimestamp = null;
        const step = (timestamp) => {
          if (!startTimestamp) startTimestamp = timestamp;
          const progress = Math.min((timestamp - startTimestamp) / duration, 1);
          const value = Math.floor(progress * (end - start) + start);
          obj.innerHTML = obj.innerHTML.replace(/\d+/, value);
          if (progress < 1) {
            window.requestAnimationFrame(step);
          }
        };
        window.requestAnimationFrame(step);
      };

      const observer = new IntersectionObserver((entries) => {
        entries.forEach(entry => {
          if (entry.isIntersecting) {
            const text = entry.target.textContent;
            const number = parseInt(text.replace(/\D/g, ''));
            if (number) {
              animateValue(entry.target, 0, number, 2000);
              observer.unobserve(entry.target);
            }
          }
        });
      });

      counters.forEach(counter => observer.observe(counter));
    }

    // Initialize all effects
    createParticleSystem();
    initMouseEffects();
    initScrollAnimations();
    initMagneticButtons();
    animateCounters();

    // Performance optimization: reduce animations on mobile
    if (window.innerWidth < 768) {
      document.querySelectorAll('.orb, .particle-container').forEach(el => {
        el.style.display = 'none';
      });
    }
  });
</script>