---
// Shallow Bay Advisors - Commercial Real Estate Brokerage
import BaseLayout from '../layouts/BaseLayout.astro';
import Navigation from '../components/Navigation.astro';
import Footer from '../components/Footer.astro';

// Property types for search filters
const propertyTypes = [
  "Warehouse",
  "Distribution Center",
  "Manufacturing",
  "Flex Space",
  "Office/Warehouse",
  "Cold Storage"
];

// Location options
const locations = [
  "Miami-Dade County",
  "Broward County",
  "Palm Beach County",
  "All South Florida"
];

// Size ranges
const sizeRanges = [
  "Under 5,000 SF",
  "5,000 - 10,000 SF",
  "10,000 - 25,000 SF",
  "25,000 - 50,000 SF",
  "50,000 - 100,000 SF",
  "100,000+ SF"
];

// Services offered
const services = [
  {
    title: "Tenant Representation",
    description: "Find the perfect warehouse or industrial space for your business needs.",
    features: ["Market Analysis", "Site Selection", "Lease Negotiation"],
    icon: "building"
  },
  {
    title: "Landlord Representation",
    description: "Maximize occupancy and value for your commercial properties.",
    features: ["Property Marketing", "Tenant Screening", "Market Positioning"],
    icon: "key"
  },
  {
    title: "Investment Sales",
    description: "Expert guidance for buying and selling commercial real estate.",
    features: ["Property Valuation", "Market Insights", "Transaction Management"],
    icon: "chart"
  }
];
