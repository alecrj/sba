  .slice(0, 3);
---

<BaseLayout
  title={`${warehouse.title} | Warehouse for Lease | Shallow Bay Advisors`}
  description={`${warehouse.description} - ${warehouse.sizeValue?.toLocaleString()} SF warehouse in ${warehouse.location}. ${warehouse.price} per SF. Available for immediate lease.`}
  image={warehouse.image}
>
  <Navigation />

  <main class="pt-20">
    <!-- Hero Section with Integrated Stats -->
    <section class="relative bg-gradient-to-br from-slate-900 via-slate-800 to-slate-900">
      <!-- Background Image with Overlay -->
      <div class="absolute inset-0">
        <img
          src={warehouse.image}
          alt={warehouse.title}
          class="w-full h-full object-cover opacity-30"
        />
        <div class="absolute inset-0 bg-gradient-to-r from-sba-navy/90 to-sba-navy/70"></div>
      </div>

      <!-- Content -->
      <div class="relative container mx-auto px-4 py-20">
        <div class="max-w-6xl mx-auto">

          <!-- Breadcrumb -->
          <nav class="mb-8 text-sm">
            <a href="/" class="text-white/70 hover:text-white">Home</a>
            <span class="mx-2 text-white/50">></span>
            <a href="/properties" class="text-white/70 hover:text-white">Properties</a>
            <span class="mx-2 text-white/50">></span>
            <span class="text-white font-medium">{warehouse.title}</span>
          </nav>

          <!-- Hero Content -->
          <div class="mb-12">
            <div class="flex items-center justify-between mb-6">
              <span class={`px-4 py-2 rounded-full text-sm font-medium ${
                warehouse.available
                  ? 'bg-green-500 text-white'
                  : 'bg-red-500 text-white'
              }`}>
                {warehouse.available ? 'Available Now' : 'Currently Leased'}
              </span>
            </div>

            <h1 class="text-4xl lg:text-6xl font-bold text-white mb-6 leading-tight">
              {warehouse.title}
            </h1>

            <div class="flex items-center text-xl text-white/80 mb-8">
              <svg class="w-6 h-6 mr-3" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z"/>
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z"/>
              </svg>
              {warehouse.address}
            </div>

            <!-- Key Stats - Floating Cards -->
            <div class="grid grid-cols-2 lg:grid-cols-4 gap-4">
              <div class="bg-white/10 backdrop-blur-md border border-white/20 p-6 rounded-xl text-center">
                <div class="text-white/70 text-sm font-medium mb-2">Total Size</div>
                <div class="text-3xl font-bold text-white">{warehouse.size}</div>
              </div>
              <div class="bg-white/10 backdrop-blur-md border border-white/20 p-6 rounded-xl text-center">
                <div class="text-white/70 text-sm font-medium mb-2">Lease Rate</div>
                <div class="text-3xl font-bold text-white">{warehouse.price}<span class="text-lg font-normal">/SF</span></div>
              </div>
              <div class="bg-white/10 backdrop-blur-md border border-white/20 p-6 rounded-xl text-center">
                <div class="text-white/70 text-sm font-medium mb-2">Property Type</div>
                <div class="text-3xl font-bold text-white">{warehouse.type}</div>
              </div>
              <div class="bg-white/10 backdrop-blur-md border border-white/20 p-6 rounded-xl text-center">
                <div class="text-white/70 text-sm font-medium mb-2">Location</div>
                <div class="text-3xl font-bold text-white">{warehouse.county}</div>
              </div>
            </div>
          </div>

        </div>
      </div>
    </section>

    <!-- Main Content -->
    <section class="py-20 bg-gradient-to-br from-gray-50 via-white to-gray-50">
      <div class="container mx-auto px-4">
        <div class="max-w-7xl mx-auto">

          <!-- Property Details -->
          <div class="grid lg:grid-cols-2 gap-16 mb-20">

            <!-- Left Column: Description & Features -->
            <div class="space-y-12">

              <!-- Description -->
              <div class="bg-white/70 backdrop-blur-xl border border-white/20 rounded-3xl p-8 shadow-xl">
                <h2 class="text-3xl font-bold text-sba-navy mb-6">Property Overview</h2>
                <p class="text-gray-700 text-lg leading-relaxed">{warehouse.description}</p>
              </div>

              <!-- Features Grid -->
              <div class="bg-white/70 backdrop-blur-xl border border-white/20 rounded-3xl p-8 shadow-xl">
                <h3 class="text-2xl font-semibold text-sba-navy mb-8">Key Features & Amenities</h3>
                <div class="grid gap-4">
                  {warehouse.features.map((feature: string) => (
                    <div class="flex items-center p-4 bg-white/50 backdrop-blur-sm rounded-2xl border border-white/30 hover:bg-white/70 transition-all duration-300">
                      <div class="w-12 h-12 bg-gradient-to-br from-green-400 to-green-600 rounded-2xl flex items-center justify-center mr-4 flex-shrink-0 shadow-lg">
                        <svg class="w-6 h-6 text-white" fill="currentColor" viewBox="0 0 20 20">
                          <path fill-rule="evenodd" d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z" clip-rule="evenodd"/>
                        </svg>
                      </div>
                      <span class="text-gray-800 font-semibold text-lg">{feature}</span>
                    </div>
                  ))}
                </div>
              </div>

              <!-- Property Gallery -->
              <div class="bg-white/70 backdrop-blur-xl border border-white/20 rounded-3xl p-8 shadow-xl">
                <h3 class="text-2xl font-semibold text-sba-navy mb-6">Property Gallery</h3>
                <div class="grid md:grid-cols-2 gap-6">
                  <div class="relative aspect-video bg-gray-200 rounded-2xl overflow-hidden shadow-lg hover:shadow-xl transition-shadow">
                    <img src={warehouse.image} alt={`${warehouse.title} - Main View`} class="w-full h-full object-cover hover:scale-105 transition-transform duration-300" />
                    <div class="absolute inset-0 bg-black/0 hover:bg-black/10 transition-colors duration-300 flex items-center justify-center opacity-0 hover:opacity-100">
                      <span class="text-white font-semibold bg-black/50 px-3 py-1 rounded-full text-sm">Main View</span>
                    </div>
                  </div>
                  <div class="relative aspect-video bg-gray-200 rounded-2xl overflow-hidden shadow-lg hover:shadow-xl transition-shadow">
                    <img src={warehouse.image} alt={`${warehouse.title} - Additional View`} class="w-full h-full object-cover hover:scale-105 transition-transform duration-300" />
                    <div class="absolute inset-0 bg-black/0 hover:bg-black/10 transition-colors duration-300 flex items-center justify-center opacity-0 hover:opacity-100">
                      <span class="text-white font-semibold bg-black/50 px-3 py-1 rounded-full text-sm">Additional View</span>
                    </div>
                  </div>
                </div>
                <p class="text-gray-600 text-sm mt-4 text-center">
                  More photos available upon request. Contact us to schedule a virtual or in-person tour.
                </p>
              </div>

            </div>

            <!-- Right Column: Contact Card -->
            <div class="space-y-8">
              <div class="bg-gradient-to-br from-sba-navy via-sba-navy-dark to-slate-900 text-white p-10 rounded-3xl shadow-2xl border border-white/10">
                <div class="mb-8">
                  <h3 class="text-2xl font-bold mb-3">Ready to Tour This Property?</h3>
                  <p class="text-blue-100 text-lg">Contact our commercial real estate experts for immediate assistance</p>
                </div>

                <div class="space-y-4 mb-8">
                  <a href="tel:954-937-9667" class="flex items-center p-4 bg-white/10 backdrop-blur-sm rounded-2xl hover:bg-white/20 transition-all duration-300 group">
                    <div class="w-12 h-12 bg-white/20 rounded-2xl flex items-center justify-center mr-4">
                      <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 5a2 2 0 012-2h3.28a1 1 0 01.948.684l1.498 4.493a1 1 0 01-.502 1.21l-2.257 1.13a11.042 11.042 0 005.516 5.516l1.13-2.257a1 1 0 011.21-.502l4.493 1.498a1 1 0 01.684.949V19a2 2 0 01-2 2h-1C9.716 21 3 14.284 3 6V5z"/>
                      </svg>
                    </div>
                    <div>
                      <div class="text-white font-bold text-xl">(954) 937-9667</div>
                      <div class="text-blue-100 text-sm">Call for immediate assistance</div>
                    </div>
                  </a>

                  <a href="mailto:contact@shallowbayadvisors.com" class="flex items-center p-4 bg-white/10 backdrop-blur-sm rounded-2xl hover:bg-white/20 transition-all duration-300 group">
                    <div class="w-12 h-12 bg-white/20 rounded-2xl flex items-center justify-center mr-4">
                      <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 8l7.89 4.26a2 2 0 002.22 0L21 8M5 19h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v10a2 2 0 002 2z"/>
                      </svg>
                    </div>
                    <div>
                      <div class="text-white font-semibold">contact@shallowbayadvisors.com</div>
                      <div class="text-blue-100 text-sm">Email our team</div>
                    </div>
                  </a>
                </div>

                <div class="border-t border-white/20 pt-6">
                  <p class="text-blue-100 text-sm">Licensed Florida Real Estate Brokerage</p>
                </div>
              </div>
            </div>

          </div>

          <!-- Full Width Booking Form Section -->
          <div class="relative">
            <!-- Background with glassmorphism -->
            <div class="absolute inset-0 bg-gradient-to-r from-sba-navy/10 via-blue-500/5 to-sba-navy/10 rounded-3xl"></div>
            <div class="relative bg-white/60 backdrop-blur-2xl border border-white/30 rounded-3xl p-12 shadow-2xl">
              <div class="text-center mb-12">
                <h2 class="text-4xl font-bold text-sba-navy mb-4">Schedule Your Property Tour</h2>
                <p class="text-xl text-gray-600 max-w-2xl mx-auto">
                  Book a personalized consultation to view {warehouse.title} and discuss your commercial real estate needs with our experts.
                </p>
              </div>

              <div class="max-w-5xl mx-auto">
                <CalendarBooking
                  title=""
                  subtitle=""
                  buttonText="Schedule Property Tour"
                  propertyId={warehouse.id.toString()}
                  propertyTitle={warehouse.title}
                />
              </div>
            </div>
          </div>

        </div>
      </div>
    </section>

    <!-- Related Properties -->
    {relatedProperties.length > 0 && (
      <section class="py-16 bg-gray-50">
        <div class="container mx-auto px-4">
          <div class="max-w-6xl mx-auto">

            <h2 class="text-2xl lg:text-3xl font-bold text-sba-navy mb-8 text-center">
              More Properties in {warehouse.county}
            </h2>

            <div class="grid md:grid-cols-3 gap-8">
              {relatedProperties.map((property: any) => (
                <a href={`/properties/${property.id}`} class="group block">
                  <div class="bg-white rounded-xl shadow-lg hover:shadow-xl transition-all duration-300 overflow-hidden">
                    <div class="relative">
                      <img
                        src={property.image}
                        alt={property.title}
                        class="w-full h-48 object-cover group-hover:scale-105 transition-transform duration-300"
                      />
                      <div class="absolute top-4 right-4">
                        <span class="bg-white bg-opacity-90 px-2 py-1 rounded text-sm font-medium text-sba-navy">
                          {property.size}
                        </span>
                      </div>
                    </div>
                    <div class="p-6">
                      <h3 class="text-lg font-semibold text-sba-navy mb-2 group-hover:text-sba-navy-dark">
                        {property.title}
                      </h3>
                      <p class="text-sba-gray text-sm mb-3">{property.location}</p>
                      <div class="flex justify-between items-center">
                        <span class="text-lg font-bold text-sba-navy">
                          {property.price}/SF
                        </span>
                        <span class="text-sm text-sba-gray">{property.type}</span>
                      </div>
                    </div>
                  </div>
                </a>
              ))}
            </div>

          </div>
        </div>
      </section>
    )}

  </main>

  <Footer />
</BaseLayout>